{
	"name": "language-runner",
	"displayName": "Language Runner",
	"author": "Password-Classified",
	"publisher": "Password-Classified",
	"version": "0.0.1",
	"description": "Runner for languages such as C, C++, R and VB.NET.",
	"engines": {
		"vscode": "^1.59.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "language-runner.run",
				"category": "Language Runner",
				"title": "Run Current File",
				"icon": {
					"light": "./run.png",
					"dark": "./run.png"
				}
			}
		],
		"languages": [
			{
				"id": "vb",
				"extensions": [
					".vb"
				],
				"aliases": [
					"VB",
					"Vb",
					"vB"
				]
			},
			{
				"id": "cpp",
				"extensions": [
					".cpp"
				],
				"aliases": [
					"CPP",
					"c++",
					"C++"
				]
			},
			{
				"id": "c",
				"extensions": [
					".c"
				],
				"aliases": [
					"C",
					"c"
				]
			},
			{
				"id": "r",
				"extensions": [
					".r"
				],
				"aliases": [
					"R",
					"r"
				]
			}
		],
		"menus": {
			"editor/title": [
				{
					"when": "resourceLangId == vb",
					"command": "language-runner.run",
					"alt": "language-runner.run",
					"group": "navigation"
				},
				{
					"when": "resourceLangId == r",
					"command": "language-runner.run",
					"alt": "language-runner.run",
					"group": "navigation"
				},
				{
					"when": "resourceLangId == cpp",
					"command": "language-runner.run",
					"alt": "language-runner.run",
					"group": "navigation"
				},
				{
					"when": "resourceLangId == c",
					"command": "language-runner.run",
					"alt": "language-runner.run",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.59.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"license": "LICENSE",
	"bugs": {
		"url": "https://github.com/Password-Classified/Language-Runner/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Password-Classified/Language-Runner"
	},
	"icon": "icon.png",
	"__metadata": {
		"installedTimestamp": 1624548889808
	}
}